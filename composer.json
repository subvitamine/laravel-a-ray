{
    "name": "subvitamine/laravel-a-ray",
    "description": "Package laravel for create push to a-ray",
    "keywords": [
        "laravel-a-ray",
        "laravel a-ray"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Subvitamine",
            "email": "contact@subvitamine.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelARay\\LaravelARay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelARay\\LaravelARay\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelARay\\LaravelARay\\LaravelARayServiceProvider"
            ],
            "aliases": {
                "LaravelARay": "LaravelARay\\LaravelARay\\LaravelARayFacade"
            }
        }
    }
}
